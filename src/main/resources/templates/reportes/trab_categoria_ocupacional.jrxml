	<queryString>
		<![CDATA[select
m.id_municipio,
m.descripcion as municipio,
en.descripcion as entidad,
c.id_categoria_ocupacional,
c.descripcion as categoria_ocupacional,
count(t.id_trabajador) as cantidad
from trabajador t
inner join categoria_ocupacional c on t.id_categoria_ocupacional = c.id_categoria_ocupacional
inner join entidad en on t.id_entidad = en.id_entidad
inner join municipio m on en.id_municipio = m.id_municipio
where ('-1' = $P{municipio} OR en.id_municipio = $P{municipio}) and ('-1' = $P{entidad} OR en.id_entidad = $P{entidad} ) and c.id_categoria_ocupacional = $P{categoria_ocupacional}
group by m.id_municipio, en.descripcion, c.id_categoria_ocupacional
order by m.id_municipio, en.descripcion, c.id_categoria_ocupacional]]>
	</queryString>
